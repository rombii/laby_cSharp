<Window x:Class="lab9.AddGrade"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:lab9"
        mc:Ignorable="d"
        Title="AddGrade" Height="450" Width="200">
    <Grid>
        <Label Content="przedmiot" HorizontalAlignment="Left"></Label>
        <TextBox Name="przedmiot" Height="20" Width="120" VerticalAlignment="Top" HorizontalAlignment="Right"></TextBox>
        <Label Content="ocena" Margin="0 30" HorizontalAlignment="Left"></Label>
        <TextBox Name="ocena" Height="20" Width="120" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0 30"></TextBox>
        <Button Content="Ok" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="20 100" Width="60" Click="okButton"></Button>
    </Grid>
</Window>
